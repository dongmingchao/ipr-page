{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ipr",
      "app",
      "kebab-case"
    ],
    "variable-name": false,
    "jsdoc-format": false
  }
}
